.dragdroplist-frame {
	width 300px
	height 40px
	border #C4C4C4 1px solid
	border-radius 10px
	margin-bottom 10px
	padding-right 10px
	display flex
	align-items center
	justify-content space-between
	cursor grabbing
}

.dragdroplist-layout {
	display flex
	flex-direction row
	align-items center
}

.dragdroplist-img-frame {
	border-radius 100%
	object-fit cover
	margin-left 10px
	margin-right 10px
}

.dragdroplist-btn {
	border none
	background-color white
}