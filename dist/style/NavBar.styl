.navbar-frame {
	background-color #FAECBB
	border-radius 100%
	display flex
	align-items center
	justify-content center
	width 40px
	height 40px
}

.navbar-frame:hover {
	transform scale(1.2, 1.2)
}

.navbar-title {
	margin-top 5px
	font-size 12px
}

.navbar-btn {
	display flex
	align-items center
	justify-content center
	flex-direction column
	width 80px
	color black
	text-decoration none
	cursor pointer
}

.navbar-layout {
	display flex
	justify-content space-around
	flex-direction row
	align-items center
	min-width 250px
}