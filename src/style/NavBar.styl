.navbar_frame {
	width: 60px;
	height: 60px;
	background-color: #FAECBB;
	border-radius: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
}

.navbar_title {
	font-size: 14px;
	margin-top: 5px;
}

.navbar_btn {
	display: flex;
	align-items: center;
	justify-content: center;
	flex-direction: column;
}

.navbar_sort {
	display: flex;
	justify-content: space-around;
	flex-direction: row;
}