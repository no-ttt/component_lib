.tab {
	overflow hidden	
}

.tab-button {
	border none
	outline none
	cursor pointer
	padding 14px 16px
	font-size 18px
	background-color white
}

.tab-border-button {
	border 1px solid black
	border-radius 5px
	margin-right 10px
	padding 4px 24px
	outline none
	cursor pointer
	font-size 18px
	background-color white
}

.tab-content {
	padding 20px
}